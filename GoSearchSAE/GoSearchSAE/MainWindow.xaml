<Window x:Class="GoSearchSAE.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GoSearchSAE"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1024">
    <Window.Resources>
        <Style TargetType="ListViewItem">
            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
        </Style>
    </Window.Resources>
    <Grid>
        <TextBox Margin="206,0,10,704"/>
        <ListView Name="listView" Margin="206,37,10,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Name" Width="256" DisplayMemberBinding="{Binding Name}"/>
                    <GridViewColumn Header="Path" Width="436" DisplayMemberBinding="{Binding Path}"/>
                    <GridViewColumn Header="Size" Width="100" DisplayMemberBinding="{Binding Size}"/>
                    <GridViewColumn Header="Last Modified" Width="100" DisplayMemberBinding="{Binding LastModified}"/>
                </GridView>
            </ListView.View>
        </ListView>
        <TreeView x:Name="treeView" Margin="0,0,823,10"/>
        
    </Grid>



</Window>
